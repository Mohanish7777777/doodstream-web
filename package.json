            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "main": "src",
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "wrangler": "^3.6.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/viperadnan-git/doodstream-web.git"
    },
